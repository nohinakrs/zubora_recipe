<div class="row">
  <div class="col-md-8 offset-md-1">
    <h1>全ユーザー一覧</h1>
    <table class="table table-hover table-inverse">
      <thead>
        <tr>
          <% @users.each do |user| %>
            <th>
              <%= user.name %> - <%= user.email %>
              <%= link_to '削除', admin_user_path(user), method: :delete, data: { confirm: '本当に削除しますか？' } %> 
            </th>
          <% end %>
        </tr>
      </thead>
    </table>
  </div>
</div>